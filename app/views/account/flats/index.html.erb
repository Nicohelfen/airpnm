<h1>Mes Flats</h1>

<% @flats.each do |flat| %>
<ul class='flat-informations'>
  <li><%= image_tag flat.picture.url(:medium) %></li>
  <li><%= link_to flat.name,  flat_path(flat) %></li>
  <li><%= flat.description %></li>
  <li><%= flat.city %></li>
  <li><%= flat.price %></li>
  <li> <%= link_to "Update", edit_account_flat_path(flat) %></li>
</ul>
<%end%>


